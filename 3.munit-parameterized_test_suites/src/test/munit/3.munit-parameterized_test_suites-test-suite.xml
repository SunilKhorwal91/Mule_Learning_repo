	xmlns:munit="http://www.mulesoft.org/schema/mule/munit" xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="3.munit-parameterized_test_suites-test-suite.xml" >
		<munit:parameterizations >
			<munit:parameterization name="dev" >
				<munit:parameters >
					<munit:parameter propertyName="firstName" value="Salman" />
					<munit:parameter propertyName="lastName" value="Khan" />
					<munit:parameter propertyName="fullName" value="Salman Khan" />
					<munit:parameter propertyName="parameterization" value="dev" />
				</munit:parameters>
			</munit:parameterization>
			<munit:parameterization name="tst" >
				<munit:parameters >
					<munit:parameter propertyName="firstName" value="Shahrukh" />
					<munit:parameter propertyName="lastName" value="Khan" />
					<munit:parameter propertyName="fullName" value="Shahrukh Khan" />
					<munit:parameter propertyName="parameterization" value="tst" />
				</munit:parameters>
			</munit:parameterization>
			<munit:parameterization name="acc" >
				<munit:parameters >
					<munit:parameter propertyName="firstName" value="Amir" />
					<munit:parameter propertyName="lastName" value="Khan" />
					<munit:parameter propertyName="fullName" value="Amir Khan" />
					<munit:parameter propertyName="parameterization" value="acc" />
				</munit:parameters>
			</munit:parameterization>
			<munit:parameterization name="prd" >
