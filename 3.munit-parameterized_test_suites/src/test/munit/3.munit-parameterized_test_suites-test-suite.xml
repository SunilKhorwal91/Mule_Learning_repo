			<munit:parameterization name="uat" >
				<munit:parameters >
					<munit:parameter propertyName="firstName" value="Cristiano" />
					<munit:parameter propertyName="lastName" value="Ronaldo" />
					<munit:parameter propertyName="fullName" value="Cristiano Ronaldo" />
					<munit:parameter propertyName="parameterization" value="uat" />
				</munit:parameters>
			</munit:parameterization>
		</munit:parameterizations>
	</munit:config>
	<munit:test name="validateFullNameSuccessTestCase" doc:id="06dc63c7-309d-4d71-9a80-85e05a536fb1" description="Test">
		<munit:execution >
			<flow-ref doc:name="Flow-ref to createFullName" doc:id="38ac0847-af33-4aa9-a043-52e15b9c8268" name="createFullName"/>
		</munit:execution>
		<munit:validation >
			<logger level="INFO" doc:name="Log Parameters" doc:id="2d4cef11-277e-41fd-9710-775c7bd7a829" message='#[%dw 2.0&#10;&#10;&#10;output application/json&#10;---&#10;{&#10;	"firstName": Mule::p("firstName"),&#10;	"lastName": Mule::p("lastName"),&#10;	"fullName": Mule::p("fullName"),&#10;	"environment_paramter": Mule::p("parameterization")&#10;}]'/>
			<munit-tools:assert-equals doc:name="Assert equals - FullName Matches" doc:id="54b1c709-acdf-4e45-9098-8758efe08cec" actual="#[payload.fullName]" expected='#[Mule::p("fullName")]' message='#["INVALID! FULL NAME DOES NOT MATCHES."]'/>
		</munit:validation>
	</munit:test>


</mule>
